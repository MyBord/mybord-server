{
  "name": "myBord-server",
  "scripts": {
    "get-schema": "graphql get-schema -p prisma",
    "remove-dist": "rm -rf dist",
    "prisma-delete:dev": "cd src/prisma/ && env-cmd -f ../../dev.env prisma delete -f",
    "prisma-deploy:dev": "cd src/prisma/ && env-cmd -f ../../dev.env prisma deploy",
    "webpack:dev": "webpack --config webpack.dev.js",
    "webpack:prod": "webpack --config webpack.prod.js",
    "build:dev": "npm-run-all remove-dist webpack:dev",
    "build:prod": "npm-run-all remove-dist webpack:prod",
    "run:dev": "env-cmd -f dev.env nodemon dist/index",
    "heroku-postbuild": "npm-run-all build:prod",
    "start": "env-cmd -f prod.env node dist/index"
  },
  "authors": [
    "Jimmy Emery"
  ],
  "engines": {
    "node": "13.5.0",
    "yarn": "1.21.1"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.7.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^12.6.2",
    "@types/webpack-env": "^1.14.1",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-eslint": "7",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-graphql": "^3.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^23.2.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.3",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.2",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "apollo-server-express": "^2.9.12",
    "axios": "^0.19.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "bcryptjs": "^2.4.3",
    "dotenv-webpack": "^1.7.0",
    "env-cmd": "^10.0.1",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-cli": "^3.0.14",
    "graphql-tag": "^2.10.1",
    "heroku": "^7.35.1",
    "jsonwebtoken": "^8.5.1",
    "prisma": "^1.34.10",
    "prisma-binding": "^2.3.16"
  }
}
